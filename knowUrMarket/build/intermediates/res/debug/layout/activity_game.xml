<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/finalbck"
    tools:context="com.example.kym.GameActivity"
    tools:ignore="MergeRootFrame" >

    <WebView
        android:id="@+id/webScroller"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true" />

    <WebView
        android:id="@+id/webViewIndex"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/webScroller"
        android:layout_centerHorizontal="true"/>

    <ProgressBar
        android:id="@+id/progressBarGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/webViewIndex"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="92dp" />

</RelativeLayout>

